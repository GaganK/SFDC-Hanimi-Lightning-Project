<aura:component controller="currentUserDetailCtrl" implements="forceCommunity:availableForAllPageTypes" access="global" >
	<aura:attribute name="userContactId" type="String"/>
    <aura:attribute name="url" type="String"/>
    <aura:attribute name="displayLabel" type="String"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
	<p style="background-color: #e81f76;
    text-align: center;
    padding: 7px 0px;" ><a href="{!v.url}" target="_blank" class="hideheader">{!v.displayLabel}</a></p>
</aura:component>