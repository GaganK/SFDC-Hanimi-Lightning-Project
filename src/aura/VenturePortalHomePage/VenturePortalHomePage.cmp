                                <article class="slds-card caption hovereffect">
                                    <figure class="slds-image slds-image--card ">
                                        <img src="{! $Resource.Mentor_CDLMentors}" alt="Description of the image" class="imgclass" />
                                        <div class="overlay">
                                            <!--  <a href="javascript:void(0);" class="slds-image__crop- -3-by-2"></a>-->
                                        </div>
                                    </figure>
                                    <div class="course-detail">
                                        <div class="course-info-box">
                                            <div class="course-teacher"><a href="#"></a></div>
                                            <a href="#">
                                                <h3 class="course-title">CDL Mentors</h3>
                                            </a> 
                                        </div>
                                    </div>
                                </article>
                                </div>
                            </a>
                        </div>
						  <div class="slds-size_1-of-1 slds-small-size_1-of-4 slds-medium-size_1-of-4 slds-p-around_medium slds-p-top_none ">
                            <div class="slds-text-align_center slds-m-around_x-small course-entry" >
                                <a href="https://www.creativedestructionlab.com/cdlsupersession/" target="_blank" class="slds-image__crop--4-by-3">
                                    <article class="slds-card caption hovereffect"> 
                                        <figure class="slds-image slds-image--card ">
                                            <img src="{! $Resource.Mentor_Session}" alt="Description of the image" class="imgclass" />
                                            <div class="overlay">
                                            </div>
                                        </figure>
                                        <div class="course-detail">
                                            <div class="course-info-box">
                                                <div class="course-teacher"><a href="https://www.creativedestructionlab.com/cdlsupersession/" target="_blank"></a></div>
                                                <a href="https://www.creativedestructionlab.com/cdlsupersession/" target="_blank">
                                                    <h3 class="course-title">Super Session 2020</h3>
                                                </a>
                                            </div>
                                        </div>
                                    </article>
                                </a>
                            </div>
                        </div> 
                        <div class="slds-size_1-of-1 slds-small-size_1-of-4 slds-medium-size_1-of-4 slds-p-around_medium slds-p-top_none ">
