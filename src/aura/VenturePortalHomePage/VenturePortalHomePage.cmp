                    <div class="slds-grid slds-wrap slds-grid_align-center" >
                        <div class="slds-size_1-of-1 slds-small-size_1-of-4 slds-medium-size_1-of-4 slds-p-around_medium slds-p-top_none   ">
                            <a onclick="{!c.redirectCDLpi}">
                                <div class="slds-text-align_center slds-m-around_x-small course-entry" >
                                    <article class="slds-card caption hovereffect">
                                        <figure class="slds-image slds-image--card  ">
                                            <img src="{! $Resource.Mentor_MyPreference}" alt="Description of the image" class="imgclass" />
                                            <div class="overlay">
                                            </div>
                                        </figure>
                                        <div class="course-detail">
                                            <div class="course-info-box">
                                                <div class="course-teacher"><a href="#"></a></div>
                                                <a href="#">
                                                    <h3 class="course-title">My Preferences</h3>
                                                </a>
                                            </div>
                                        </div>  
                                    </article>
                                </div>
                            </a>
                        </div> 
                        <div class="slds-size_1-of-1 slds-small-size_1-of-4 slds-medium-size_1-of-4 slds-p-around_medium slds-p-top_none">
