                    <div style="display: block;"><p class="resourcecaption">Resources</p> 
                        <div class="resource">
                            <div>
                                <a href="{!v.loggedinUserSlackUrl}" target="_blank">
                                    <i class="fa fa-slack" aria-hidden="true"></i><br/>
                                    Join Slack 
                                </a>
                            </div>
                            <div>
                                <a href="/VenturePortal/s/scheduleinterface" target="_blank">
                                    <i class="fa fa-clock-o"></i><br/>
                                    My Schedule 
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</aura:component>