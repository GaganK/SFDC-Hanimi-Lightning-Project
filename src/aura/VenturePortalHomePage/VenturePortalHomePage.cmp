<aura:component implements="forceCommunity:availableForAllPageTypes" access="global" controller="SlackDetailCtrl">

    <aura:attribute name="loggedinUserSlackUrl" type="string"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <ltng:require styles="{!$Resource.cssresource+'/css-resources/css/style.css'}"/>
    <ltng:require styles="{!$Resource.fontIcon+'/font-icons/css/font-awesome.min.css'}"/>

    <div style="background-color: transparent; padding:20px 0px;">
        <div class="slds-grid slds-wrap">
            <div class="slds-size_1-of-1 slds-small-size_1-of-1 slds-medium-size_10-of-12" >
