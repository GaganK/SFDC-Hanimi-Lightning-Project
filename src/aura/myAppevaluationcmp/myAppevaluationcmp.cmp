                                <aura:iteration items="{!v.options}" var="option">
                                    <option text="{!option.label}" value="{!option.value}" selected="{!option.selected}"/>
                                </aura:iteration>
                            </lightning:select>
                        </div>
                        <div class="gSearch slds-media__body" style="margin-right: var(--lwc-spacingSmall,0.75rem);">
