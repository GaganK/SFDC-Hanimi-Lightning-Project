    <aura:if isTrue="{!v.isProcessing}">
        <div class="slds-spinner_container">
            <div class="slds-spinner--brand slds-spinner slds-spinner--small" role="alert">
                <span class="slds-assistive-text">Loading...</span>
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
    </aura:if>
    <div aura:id="columnSelectionModal" class="customModal hideModal">
        <aura:if isTrue="{!v.iscolumnsSelOpen}">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                     aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open picklist-modal">
                <div class="slds-modal__container">
                    <!-- Modal/Popup Box Header Starts here-->
                    <header class="slds-modal__header">
                        <lightning:buttonIcon iconName="utility:close" onclick="{!c.closeLookupModal}"
                                              alternativeText="close" variant="bare-inverse" class="slds-modal__close" />
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Select columns to
                            display</h2>
                    </header>
                    
                    <!--Modal/Popup Box Body Starts here-->
                    <div class="slds-modal__content slds-p-around_small" id="modal-content-id-1">
                        <div style="width: 90%;margin: auto;">
                            <lightning:dualListbox name="multipleOptions" sourceLabel="Available"
                                                   selectedLabel="Selected" options="{!v.availableColumns}" value="{!v.selectedColumns}"
                                                   requiredOptions="{!v.requiredColumns}" disableReordering="true"
                                                   onchange="{!c.onColumnChange}" />
                        </div>
                    </div>
                    
                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer">
                        <lightning:button variant="neutral" label="Cancel" title="Cancel"
                                          onclick="{!c.closeColumnsModel}" />
                        <lightning:button variant="brand" label="OK" title="OK" onclick="{!c.updateColumns}" />
                    </footer>
                </div>
            </section>
        </aura:if>
    </div>
    <aura:if isTrue="{!v.isModalOpen}">
        <!-- Modal/Popup Box starts here-->
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container" style="height: 460px;width:50% !important;">
                <!-- Modal/Popup Box Header Starts here-->
                <header class="slds-modal__header">
                    <lightning:buttonIcon iconName="utility:close"
                                          onclick="{! c.closeModel }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close"/>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate" >
                        <p style="color: rgba(156, 27, 83, 1) !important;">Evaluation Filters</p>
                    </h2>
                </header>
                <!--Modal/Popup Box Body Starts here-->
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    
                       
                       <div class="filterBox">
                            <lightning:input aura:id="gSearch" placeholder="Search this list..." type="search"
                                            value="{!v.fieldSearchTerm}" tabindex="0" onchange="{!c.onFieldSearch}"/>
                            
                                <lightning:checkboxGroup options="{!v.availableOptions}" value="{!v.filteredOptions}"/>
                            
                        </div>
                        
                        
                        
                    
                </div> 
                <!--Modal/Popup Box Footer Starts here-->
                <footer class="slds-modal__footer">
                    <lightning:button variant="neutral"
                                      label="Cancel"
                                      title="Cancel"
                                      onclick="{! c.closeModel }"/>
                    <lightning:button variant="brand" label="Submit" onclick="{!c.getApplicationRecords}" />
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    <div class="slds-grid slds-wrap fonttype">
        <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12" style="padding-right:7px !important;">
