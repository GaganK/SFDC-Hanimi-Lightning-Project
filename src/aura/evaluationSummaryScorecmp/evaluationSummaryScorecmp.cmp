								class="{!v.selectedColumnsInfo.primaryContact ? 'showColumn' : 'hiddenColumn'}">
								<div class="slds-clearfix" style="display: inline-flex;">
									<div class="{!v.selectedColumnsInfo.primaryContact ? 'slds-truncate' : 'slds-hide'}"
										title="Primary Contact" style="cursor: pointer;">
