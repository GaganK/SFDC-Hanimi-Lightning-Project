                                    First Site Preference
                                    <aura:if isTrue="{!v.sortField=='X1st_Preference_CDL_Location__c'}">
                                        <span>
                                            <aura:if isTrue="{!v.sortAsc}">
                                                <lightning:icon iconName="utility:arrowdown" size="xx-small" />
                                                <aura:set attribute="else">
                                                    <lightning:icon iconName="utility:arrowup" size="xx-small" />
                                                </aura:set>
                                            </aura:if>
                                        </span>
                                    </aura:if>
									
                                </div>
                                <lightning:buttonIcon aura:id="firstsitepreference" iconName="utility:filterList" ariaLabel="First Site Preference"
                                                                              onclick="{! c.openModel }" alternativeText="openFilter" class="filtericon"/>
                                <div class="slds-float_right hideShowIcon">
                                    <lightning:buttonIcon
                                        iconName="{!v.selectedColumnsInfo.firstSitePreference ? 'utility:hide' : 'utility:preview'}"
                                        variant="bare" onclick="{!c.showHideColumn}" ariaLabel="firstSitePreference"
                                        tooltip="{!v.selectedColumnsInfo.firstSitePreference ? 'Hide First Site Preferance' : 'Show First Site Preferance'}" />
                                </div>
                            </div>
                        </th>
						 <th scope="col"
                            class="{!v.selectedColumnsInfo.secondSitePreference ? 'showColumn' : 'hiddenColumn'}">
                            <div class="slds-clearfix" style="display: inline-flex;">
                                <div onclick="{!c.sortBySecondSitePreference}"
                                    class="{!v.selectedColumnsInfo.secondSitePreference ? 'slds-truncate' : 'slds-hide'}"
