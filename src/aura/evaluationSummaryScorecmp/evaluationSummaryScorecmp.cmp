									<div class="{!v.selectedColumnsInfo.streamPotential ? 'slds-truncate' : 'slds-hide'}"
										title="Stream Potential" style="cursor: pointer;">
										Stream Potential
										<lightning:buttonIcon aura:id="streamPotential" iconName="utility:filterList"
                                              onclick="{! c.openModel }"
                                              alternativeText="openFilter"
                                              ariaLabel="Stream Potential"                 
