<aura:component controller="evaluationSummaryScoreCtrl" implements="flexipage:availableForAllPageTypes,force:hasRecordId,force:appHostable">
	<!--Init handler which is call initRecords js function on component Load-->
	 <aura:attribute name="availableColumns" type="List" default="[]" />
    <aura:attribute name="selectedColumns" type="List" default="[]" />
    <aura:attribute name="requiredColumns" type="List" default="[]" />
    <aura:attribute name="selectedColumnsInfo" type="object" default="{}" />
    <aura:attribute name="columnMapping" type="object" default="{}" />
	<aura:handler name="init" value="{!this}" action="{!c.initRecords}"/>
	<aura:attribute name="UpdatedApp" type="Application__c"/>
	<aura:attribute name="isSearching" type="boolean" default="false " />
	<aura:attribute type="String" name="searchTerm" default=""/>
	<aura:attribute name="isProcessing" type="boolean" default="true" />
	<!--declare aura attributes-->
	<aura:attribute type="String" name="sortField" default="name"/>
	<aura:attribute type="Boolean" name="sortAsc" default="false"/>
	<aura:attribute type="Boolean" name="sortAscFirstSitePreference" default="false"/>
	<aura:attribute name="PaginationList" type="Application__c[]"/>
	<aura:attribute name="ApplicationList" type="Application__c[]" description="store Application records list"/>
	<aura:attribute name="selectedApplications" type="List" default="[]"/>
	<aura:attribute name="showSaveCancelBtn" type="boolean" default="false" description="flag for rendered save and cancel buttons in aura:if "/>
	<aura:handler name="selectedAppId" event="c:evaluationSummaryScoreEvent" action="{!c.selectedRow}"/>
	<aura:handler name="UpdatedRecs" event="c:evaluationSummaryScoreUpdateEvnt" action="{!c.setUpdateRecords}"/>
	<aura:attribute name="iscolumnsSelOpen" type="boolean" default="false"/>
	<aura:attribute name="clickedButtonId" type="string" default=""/>
	<aura:attribute name="isModalOpen" type="boolean" default="false"/>
	<aura:attribute name="ApplList" type="List" default="[]"/>
	<aura:attribute name="firstsitepreferenceList" type="List" default="[]"/>
	<aura:attribute name="selectfirstsitepreference" type="List" default="[]"/>
	<aura:attribute name="secondsitepreferenceList" type="List" default="[]"/>
	<aura:attribute name="selectedsecondsitepreference" type="List" default="[]"/>
	<aura:attribute name="VentureList" type="List" default="[]"/>
	<aura:attribute name="selectedVentures" type="List" default="[]"/>
	<aura:attribute name="StreamPotentialList" type="List" default="[]"/>
	<aura:attribute name="selectedStreamPotentialList" type="List" default="[]"/>
	<aura:attribute name="refferalList" type="List" default="[]"/>
	<aura:attribute name="selectreferal" type="List" default="[]"/>
	<aura:attribute name="finalrankList" type="List" default="[]"/>
	<aura:attribute name="selectfinalrank" type="List" default="[]"/>
	<aura:attribute name="interviewRecommendationList" type="List" default="[]"/>
	<aura:attribute name="selectinterviewRecommendation" type="List" default="[]"/>
	<aura:attribute name="streamRecommendationList" type="List" default="[]"/>
	<aura:attribute name="selectstreamRecommendation" type="List" default="[]"/>
	<aura:attribute name="techScreeningRecommendationList" type="List" default="[]"/>
	<aura:attribute name="selecttechScreeningRecommendation" type="List" default="[]"/>
	<aura:attribute name="startPage" type="Integer" />
	<aura:attribute name="endPage" type="Integer"/>
	<aura:attribute name="totalRecordsCount" type="Integer"/>
	<aura:attribute name="pageSize" type="Integer" default="8"
                    description="number of records to be display on per page"/>
	<aura:attribute name="currentPage" type="integer" default="1"/>
	<aura:attribute name="totalPagesCount" type="integer"/>
    <aura:attribute name="isAppTypeSelSelf" type="boolean" default="false"/>
	<aura:attribute name="currentselectedAppType" type="string" default="All"/>
    
     <aura:attribute name="allApplicationNumbers" type="List" default="[]" />
    <aura:attribute name="allAvailableOptions" type="List" default="[]" />
    
    <aura:attribute name="availableOptions" type="List" default="[]" />
    <aura:attribute name="filteredOptions" type="List" default="[]" />
    <aura:attribute name="filterBy" type="String" default="" />
    <aura:attribute name="fieldSearchTerm" type="String" default=""/>
	 <aura:attribute name="filteredFirstSitePreference" type="List"  default="[]"/>
    <aura:attribute name="filteredSecondSitePreference" type="List" default="[]" />
    <aura:attribute name="filteredStreamPotentials" type="List" default="[]" />
    <aura:attribute name="filteredVentures" type="List" default="[]" />
    <aura:attribute name="filteredApplicationNumbers" type="List" default="[]" />
     <aura:attribute name="filteredReferralBy" type="List" default="[]" />
     <aura:attribute name="filteredInterviewRecommendation" type="List" default="[]" />
    <aura:attribute name="filteredStreamRecommendation" type="List" default="[]" />
    <aura:attribute name="filteredTechStreamRecommendation" type="List" default="[]" />
     <aura:attribute name="filteredSiteReferral" type="List" default="[]" />
     <aura:attribute name="filteredReasonRecommendation" type="List" default="[]" />
    
    <aura:attribute name="teamrank" type="List" default="[]" />
    <aura:attribute name="filteredTeamRank" type="List" default="[]" />
    <aura:attribute name="selectTeamrank" type="List" default="[]" />
    
    <aura:attribute name="techSpecial" type="List" default="[]" />
    <aura:attribute name="filteredtechSpecial" type="List" default="[]" />
    <aura:attribute name="selectTechSpecial" type="List" default="[]" />
    
    
    <aura:attribute name="techScreeningRec" type="List" default="[]" />
    <aura:attribute name="filteredtechScreeningRec" type="List" default="[]" />
    
	<aura:if isTrue="{!v.isProcessing}">
	<div class="slds-spinner_container">
		<div class="slds-spinner--brand slds-spinner slds-spinner--small" role="alert">
			<span class="slds-assistive-text">Loading...</span>
			<div class="slds-spinner__dot-a"></div>
			<div class="slds-spinner__dot-b"></div>
		</div>
	</div>
</aura:if>
	<aura:if isTrue="{!v.isModalOpen}">
		<!-- Modal/Popup Box starts here-->
		<section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
			<div class="slds-modal__container" style="height: 460px;width:50% !important;">
				<!-- Modal/Popup Box Header Starts here-->
				<header class="slds-modal__header">
					<lightning:buttonIcon iconName="utility:close"
                                          onclick="{!c.closeModel }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close"/>
					<h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate" >
						<p style="color: rgba(156, 27, 83, 1) !important;">Evaluation Filters</p>
					</h2>
				</header>
				<!--Modal/Popup Box Body Starts here-->
				<div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
					  <div class="filterBox">
                            <lightning:input aura:id="gSearch" placeholder="Search this list..." type="search"
                                            value="{!v.fieldSearchTerm}" tabindex="0" onchange="{!c.onFieldSearch}"/>
                            
                                <lightning:checkboxGroup options="{!v.availableOptions}" value="{!v.filteredOptions}"/>
                            
                        </div>
				</div>
				<!--Modal/Popup Box Footer Starts here-->
				<footer class="slds-modal__footer">
					<lightning:button variant="neutral"
                                      label="Cancel"
                                      title="Cancel"
                                      onclick="{! c.closeModel }"/>
					<lightning:button variant="brand" label="Submit" onclick="{!c.getApplicationRecords}" />
				</footer>
			</div>
		</section>
		<div class="slds-backdrop slds-backdrop_open"></div>
	</aura:if>
<div aura:id="columnSelectionModal" class="customModal hideModal">
        <aura:if isTrue="{!v.iscolumnsSelOpen}">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open picklist-modal">
                <div class="slds-modal__container">
                    <!-- Modal/Popup Box Header Starts here-->
                    <header class="slds-modal__header">
                        <lightning:buttonIcon iconName="utility:close" onclick="{!c.closeLookupModal}"
                            alternativeText="close" variant="bare-inverse" class="slds-modal__close" />
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Select columns to
                            display</h2>
                    </header>

                    <!--Modal/Popup Box Body Starts here-->
                    <div class="slds-modal__content slds-p-around_small" id="modal-content-id-1">
                        <div style="width: 90%;margin: auto;">
                            <lightning:dualListbox name="multipleOptions" sourceLabel="Available"
                                selectedLabel="Selected" options="{!v.availableColumns}" value="{!v.selectedColumns}"
                                requiredOptions="{!v.requiredColumns}" disableReordering="true"
                                onchange="{!c.onColumnChange}" />
                        </div>
                    </div>

                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer">
                        <lightning:button variant="neutral" label="Cancel" title="Cancel"
                            onclick="{!c.closeColumnsModel}" />
                        <lightning:button variant="brand" label="OK" title="OK" onclick="{!c.updateColumns}" />
                    </footer>
                </div>
            </section>
        </aura:if>
    </div>
<div class="slds-grid slds-wrap fonttype">
		<div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12" style="padding-right:7px !important;">
