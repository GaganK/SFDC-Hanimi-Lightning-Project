									</div>
									<div class="slds-float_right hideShowIcon">
										<lightning:buttonIcon
											iconName="{!v.selectedColumnsInfo.primaryContact ? 'utility:hide' : 'utility:preview'}"
											variant="bare" onclick="{!c.showHideColumn}" ariaLabel="primaryContact"
											tooltip="{!v.selectedColumnsInfo.primaryContact ? 'Hide Primary Contact' : 'Show Primary Contact'}" />
									</div>
								</div>
							</th>							
									
							
							</tr>
						</thead>
						<tbody>
							<aura:iteration items="{!v.PaginationList}" var="app">
								<c:evaluationSummaryScoreChildcmp singleEva="{!app.Evaluations__r}" singleRec="{!app}" selectedColumnsOptions="{!v.selectedColumnsInfo}"
                                     showSaveCancelBtn="{!v.showSaveCancelBtn}"
                                      />
							</aura:iteration>
						</tbody>
					</table>
